msgid "@niko Everyone in this room is fine!"
msgstr "@niko 這個房間所有人都很好！"

#: Maps/FINALE/Credits/EV070:1:19:text Maps/FINALE/Credits/map events:5:25:text
msgid "@niko_speak You'll be able to go back home soon..."
msgstr "@niko_speak 你很快就能回家了..."

#: Maps/FINALE/Credits/EV070:1:20:text Maps/FINALE/Credits/EV070:1:48:text
msgid "@plight_sigh Oh thank goodness gracious."
msgstr "@plight_sigh 哦，感謝上帝，天哪。"

#: Maps/FINALE/Credits/EV070:1:21:text Maps/FINALE/Credits/EV070:1:49:text
msgid "@plight_worry I can't work when I'm dead!"
msgstr "@plight_worry 我死了就不能工作了！"

#: Maps/FINALE/Credits/EV070:1:25:text
msgid "@niko6 (...\\p, IS he?)"
msgstr "@niko6 （...\\p，他還好嗎？）"

#: Maps/FINALE/Credits/EV070:1:26:choice:0
msgid "He's fine, Niko"
msgstr "他很好，Niko"

#: Maps/FINALE/Credits/EV070:1:30:choice:0
msgid "Everyone here is fine."
msgstr "這裏所有人都很好。"

#: Maps/FINALE/Credits/EV070:1:34:choice:0 Maps/FINALE/Credits/map events:5:39:choice:0
