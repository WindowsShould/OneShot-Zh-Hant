msgstr "fuck,shit,ass,cock,damn,penis,tiddy,titty,cunt,whore"

msgid "NAME_NIKOS"
msgstr "niko"

msgid "NAMES_LIKE_NIKO"
msgstr "nico,nikko,nicco,nikoh,nicoh"

msgid "NAMES_LIKE_MOMDAD"
msgstr "mom,dad,mommy,daddy,mama,papa"

